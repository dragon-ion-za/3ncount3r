{
  "name": "spa-charact3r",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/spa-charact3r/src",
  "tags": [],
  "targets": {
    "buildMfe": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx run spa-charact3r:buildBlazor",
            "forwardAllArgs": false
          },
          {
              "command": "nx run spa-charact3r:build",
              "forwardAllArgs": false
          },
          {
              "command": "exit",
              "forwardAllArgs": false
          }
        ]
      },
      "parallel": false
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/spa-charact3r",
        "compiler": "swc",
        "main": "apps/spa-charact3r/src/main.ts",
        "tsConfig": "apps/spa-charact3r/tsconfig.app.json",
        "webpackConfig": "apps/spa-charact3r/webpack.config.js",
        "assets": [
          "apps/spa-charact3r/src/favicon.ico",
          "apps/spa-charact3r/src/assets",
          "apps/spa-charact3r/src/_framework",
          "apps/spa-charact3r/src/_content"
        ],
        "index": "apps/spa-charact3r/src/index.html",
        "baseHref": "/",
        "styles": ["apps/spa-charact3r/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "apps/spa-charact3r/src/environments/environment.ts",
              "with": "apps/spa-charact3r/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serveMfe": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx run spa-charact3r:buildBlazor",
            "forwardAllArgs": false
          },
          {
              "command": "nx run spa-charact3r:serve",
              "forwardAllArgs": false
          }
        ]
      },
      "parallel": false
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "spa-charact3r:build",
        "port": 4202
      },
      "configurations": {
        "production": {
          "buildTarget": "spa-charact3r:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/spa-charact3r/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/spa-charact3r/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "buildBlazor": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx run doner-lib:publish",
            "forwardAllArgs": false
          },
          {
            "command": "xcopy /E /I \".\\dist\\libs\\doner-lib\\net7.0\\publish\\wwwroot\\_framework\\\" \".\\apps\\spa-charact3r\\src\\_framework\\\"",
            "forwardAllArgs": false
          },
          {
            "command": "xcopy /E /I \".\\dist\\libs\\doner-lib\\net7.0\\publish\\wwwroot\\_content\\\" \".\\apps\\spa-charact3r\\src\\_content\\\"",
            "forwardAllArgs": false
          }
        ]
      },
      "parallel": false
    }
  }
}
